{
  "expo": {
    "name": "Marketspace",
    "slug": "marketspace",
    "scheme": "marketspace",
    "version": "1.0.0",
    "orientation": "portrait",
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.brunodsazevedo.marketspace"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
